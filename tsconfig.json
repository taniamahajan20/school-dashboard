{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "dist",
    "rootDir": "src",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
     "strict": true
  },
  "include": [ "src" ],
  "exclude": ["node_modules","dist"]
}
